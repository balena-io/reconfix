{
  "config_txt": {
    "type": "ini",
    "location": {
      "path": "config.txt",
      "partition": {
        "primary": 1
      }
    },
    "properties": {
      "gpuMem": {
        "type": [ "number" ],
        "mapping": [
          [ "gpu_mem" ]
        ]
      }
    }
  },
  "network_config": {
    "type": "ini",
    "location": {
      "parent": "config_json",
      "property": [ "files", "network/network.config" ]
    },
    "properties": {
      "networkSsid": {
        "type": [ "string" ],
        "mapping": [
          [ "service_home_wifi", "Name" ]
        ]
      },
      "networkKey": {
        "type": [ "string" ],
        "mapping": [
          [ "service_home_wifi", "Passphrase" ]
        ]
      },
      "networkType": {
        "type": [ "string" ],
        "mapping": [
          {
            "value": "ethernet",
            "template": {
              "service_home_ethernet": {
                "Type": "ethernet",
                "Nameservers": "8.8.8.8,8.8.4.4"
              }
            }
          },
          {
            "value": "wifi",
            "template": {
              "service_home_ethernet": {
                "Type": "ethernet",
                "Nameservers": "8.8.8.8,8.8.4.4"
              },
              "service_home_wifi": {
                "Hidden": true,
                "Type": "wifi",
                "Name": "[[string]]",
                "Passphrase": "[[string]]",
                "Nameservers": "8.8.8.8,8.8.4.4"
              }
            }
          }
        ]
      }
    }
  },
  "config_json": {
    "type": "json",
    "location": {
      "path": "config.json",
      "partition": {
        "primary": 4,
        "logical": 1
      }
    },
    "properties": {
      "appUpdatePollInterval": {
        "type": [ "string" ],
        "mapping": [
          [ "appUpdatePollInterval" ]
        ]
      }
    }
  }
}
